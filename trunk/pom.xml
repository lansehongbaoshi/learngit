<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <name>Chsi knowledge</name>
    <modelVersion>4.0.0</modelVersion>
    <groupId>com.chsi</groupId>
    <artifactId>chsi_package_knowledge</artifactId>
    <packaging>pom</packaging>
    <modules>
        <module>knowledge</module>
        <module>application</module>
        <module>webapp</module>
    </modules>
    <version>1.0.0</version>
    <scm>
        <connection>scm:svn:https://svn.chsi.com.cn/svn/repos/chsicommunity/knowledge/trunk</connection>
        <developerConnection>scm:svn:https://svn.chsi.com.cn/svn/repos/chsicommunity/knowledge/trunk</developerConnection>
    </scm>
    <distributionManagement>
        <repository>
            <id>Chsi Repository</id>
            <name>Repository Name</name>
            <url>ftp://maven.chsi.com.cn</url>
        </repository>
        <snapshotRepository>
            <id>Chsi Repository</id>
            <name>Repository Name</name>
            <url>ftp://maven.chsi.com.cn</url>
        </snapshotRepository>
    </distributionManagement>
    <build>
        <sourceDirectory>src/main/java</sourceDirectory>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>2.3.2</version>
                <configuration>
                    <source>1.6</source>
                    <target>1.6</target>
                    <encoding>UTF-8</encoding>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-resources-plugin</artifactId>
                <version>2.2</version>
                <configuration>
                    <encoding>UTF-8</encoding>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-eclipse-plugin</artifactId>
                <version>2.7</version>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-surefire-plugin</artifactId>
                <version>2.4.2</version>
                <configuration>
                    <skipTests>false</skipTests>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-scm-plugin</artifactId>
                <version>1.8</version>
                <configuration>
                    <connectionType>developerConnection</connectionType>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-release-plugin</artifactId>
                <version>2.3.2</version>
                <!-- <configuration>
                    <tagBase>https://svn.chsi.com.cn/svn/repos/xlrz/wssq/tags</tagBase>
                    </configuration> -->
            </plugin>
        </plugins>
        <extensions>
            <extension>
                <groupId>org.apache.maven.wagon</groupId>
                <artifactId>wagon-ftp</artifactId>
                <version>2.2</version>
            </extension>
        </extensions>
    </build>
    <dependencies>
        <dependency>
            <groupId>junit</groupId>
            <artifactId>junit</artifactId>
            <version>4.4</version>
            <scope>test</scope>
        </dependency>
        <dependency>
            <groupId>com.chsi</groupId>
            <artifactId>chsi-search-client</artifactId>
            <version>${chsi-search-client.version}</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>log4j</groupId>
            <artifactId>log4j</artifactId>
            <version>1.2.14</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>com.chsi</groupId>
            <artifactId>chsi-contact-client</artifactId>
            <version>${chsi-contact-client.version}</version>
            <scope>provided</scope>
        </dependency>
    </dependencies>
    <repositories>
        <repository>
            <id>chsi</id>
            <name>Chsi Repository</name>
            <layout>default</layout>
            <url>http://maven.chsi.com.cn/maven2</url>
            <snapshots>
                <enabled>true</enabled>
            </snapshots>
        </repository>
        <repository>
            <id>central</id>
            <name>Maven Repository Switchboard</name>
            <layout>default</layout>
            <url>http://repo1.maven.org/maven2</url>
            <snapshots>
                <enabled>false</enabled>
            </snapshots>
        </repository>
        <repository>
            <id>maven2-repository.dev.java.net</id>
            <name>Java.net Repository for Maven</name>
            <url>http://download.java.net/maven/2/</url>
            <layout>default</layout>
        </repository>
        <repository>
            <id>jboss-repository</id>
            <name>Jboss Repository for Maven</name>
            <url>http://repository.jboss.org/maven2</url>
            <layout>default</layout>
        </repository>
    </repositories>
    <properties>
        <resource_filters_sub>remote</resource_filters_sub>
        <cas.version>3.3.1</cas.version>
        <commons-logging.version>1.1.1</commons-logging.version>
        <commons-lang.version>2.4</commons-lang.version>
        <commons-beanutils.version>1.7.0</commons-beanutils.version>
        <commons-collections.version>2.1.1</commons-collections.version>
        <commons-digester.version>1.7</commons-digester.version>
        <commons-validator.version>1.1.4</commons-validator.version>
        <commons-httpclient.version>3.1</commons-httpclient.version>
        <commons-fileupload.version>1.3.1.chsi.3</commons-fileupload.version>
        <commons-compress.version>1.9</commons-compress.version>
        <!-- struts2 -->
        <spring-core.version>3.1.2.RELEASE</spring-core.version>
        <spring-web.version>3.1.2.RELEASE</spring-web.version>
        <spring-webmvc.version>3.1.2.RELEASE</spring-webmvc.version>
        <spring-security-core.version>2.0.4</spring-security-core.version>
        <spring-security-core-tiger.version>2.0.4</spring-security-core-tiger.version>
        <spring-security-cas-client.version>2.0.4</spring-security-cas-client.version>
        <standard.version>1.1.2</standard.version>
