        <struts2-tiles-plugin.version>2.3.15.2</struts2-tiles-plugin.version>
        <xwork-core.version>2.3.15.2.chsi</xwork-core.version>
        <tiles-api.version>2.0.6</tiles-api.version>
        <tiles-core.version>2.0.6</tiles-core.version>
        <tiles-jsp.version>2.0.6</tiles-jsp.version>
        <freemarker.version>2.3.19</freemarker.version>

        <hibernate-core.version>4.1.7.Final</hibernate-core.version>

