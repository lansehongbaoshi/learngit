<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>

    <package name="htgl.tag" extends="knowledge-default" namespace="/htgl/tag">
    <interceptors>
          <interceptor-stack name="knowledgeStack">
              <interceptor-ref name="chsiDefaultStack"></interceptor-ref>
              <interceptor-ref name="store">
                  <param name="operationMode">AUTOMATIC</param>
              </interceptor-ref>
          </interceptor-stack>
        </interceptors>
        
        <action name="getTagList" class="tagActionBean" method="getTagList">
            <!--  <result name="success" type="tiles">htgl.tag.tagList</result> -->
        </action>
        
    </package>
</struts>