<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>

    <package name="htgl.knowledge" extends="knowledge-default" namespace="/htgl/knowledge">
       
        <interceptors>
          <interceptor-stack name="knowledgeStack">
              <interceptor-ref name="chsiDefaultStack"></interceptor-ref>
              <interceptor-ref name="store">
                  <param name="operationMode">AUTOMATIC</param>
              </interceptor-ref>
          </interceptor-stack>
        </interceptors>
        <!--       
        <action name="upload" class="com.chsi.knowledge.action.upload.UploadAction" method="upload">
        </action> -->
        
        <action name="getKnowledgeList" class="knowledgeActionBean" method="getKnowledgeList">
            <result name="success" type="tiles">htgl.knowledge.knowledgeList</result>
        </action>
        
        <action name="getKnowledge" class="knowledgeActionBean" method="getKnowledge">
            <result name="success" type="tiles">htgl.knowledge.knowledge</result>
        </action>
        
        
    </package>
</struts>